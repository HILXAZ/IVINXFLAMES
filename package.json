    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "framer-motion": "^10.16.16",
    "howler": "^2.2.4",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.294.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "react-spring": "^9.7.4",
    "react-timer-hook": "^4.0.5",
    "recharts": "^2.8.0",
    "three": "^0.180.0",
