{
  "name": "addiction-control-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server/index.js",
    "dev:all": "concurrently \"vite\" \"node server/index.js\"",
    "mentor": "node server/mentor/server.js",
    "dev:mentor": "concurrently \"vite\" \"node server/mentor/server.js\"",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "setup": "node scripts/setup.js",
    "generate-icons": "node scripts/generate-icons.js",
    "test": "echo 'No tests configured yet'",
    "deploy": "npm run build && echo 'Ready for deployment'",
    "analyze": "npm run build && npx vite-bundle-analyzer"
  },
  "dependencies": {
    "@google/generative-ai": "^0.11.2",
